{
  "mappings-item-schema": {
    "label": "description",
    "type": "color|text",
    "udlSelector": "single xml selector (that maps to cssSelectors, e.g. WordsStyle[name='COMMENTS']). Can be empty (as is the case for default background)",
    "udlAttr": "single attr to change, e.g. bgColor",
    "cssSelectors": ["array", "of", "css", "selectors", "(relative to preview.html body)"],
    "cssAttr": "single attr to change, e.g. background-color"
  },
  "mappings": [
    {
      "label": "language name",
      "type": "text",
      "udlSelector": "UserLang",
      "udlAttr": "name"
    },
    {
      "label": "global background color",
      "type": "color",
      "cssSelectors": ["div:first-of-type", ""],
      "cssAttr": "background-color"
    },
    {
      "label": "default text color",
      "type": "color",
      "udlSelector": "WordsStyle[name='DEFAULT']",
      "udlAttr": "fgColor",
      "cssSelectors": [".sc0"],
      "cssAttr": "color"
    },
    {
      "label": "string \"\" (delimiters2)",
      "type": "color",
      "udlSelector": "WordsStyle[name='DELIMITERS2']",
      "udlAttr": "fgColor",
      "cssSelectors": [".sc17"],
      "cssAttr": "color"
    },
    {
      "label": "string interp '' (delimiters1)",
      "type": "color",
      "udlSelector": "WordsStyle[name='DELIMITERS1']",
      "udlAttr": "fgColor",
      "cssSelectors": [".sc16"],
      "cssAttr": "color"
    },
    {
      "label": "numbers (decimals, floats and hex)",
      "type": "color",
      "udlSelector": "WordsStyle[name='NUMBERS']",
      "udlAttr": "fgColor",
      "cssSelectors": [".sc3"],
      "cssAttr": "color"
    },
    {
      "label": "keywords: function,break,case,for,var... (kw1)",
      "type": "color",
      "udlSelector": "WordsStyle[name='KEYWORDS1']",
      "udlAttr": "fgColor",
      "cssSelectors": [".sc4"],
      "cssAttr": "color"
    },
    {
      "label": "regex ~// (delimiters3)",
