      "udlSelector": "WordsStyle[name='DELIMITERS3']",
      "udlAttr": "fgColor",
      "cssSelectors": [".sc18"],
      "cssAttr": "color"
    },
    {
      "label": "operators:',>,&,|,%,... (operators)",
      "type": "color",
